{
  "name": "@cenk1cenk2/renovate-config",
  "version": "2.3.148",
  "description": "Renovate configuration generator for repositories.",
  "author": "Cenk Kilic",
  "license": "GPL-3.0-or-later",
  "repository": "https://gitlab.kilic.dev/renovate/renovate-config",
  "type": "module",
  "bin": {
    "renovate-config-generator": "./bin/run.js"
  },
  "files": [
    "/config",
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "dev": "./bin/dev.js",
    "start": "./bin/run.js",
    "build": "tsdown",
    "dev:start": "tsdown --watch src/",
    "format": "prettier --log-level warn --write ./ && eslint --fix ./src",
    "lint": "eslint ./src",
    "manifest": "oclif manifest"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "./*.{ts,js,tsx,jsx}": [
      "prettier --log-level warn --write",
      "eslint --fix"
    ],
    "./*.{json,md}": [
      "prettier --log-level warn --write"
    ]
  },
  "oclif": {
    "bin": "renovate-config-generator",
    "dirname": "renovate-config-generator",
    "commands": {
      "strategy": "single",
      "target": "./dist/commands/index.mjs"
    },
    "plugins": [],
    "hooks": {}
  },
  "dependencies": {
    "@cenk1cenk2/oclif-common": "^7.0.7",
    "@listr2/manager": "^4.1.0",
    "@oclif/core": "^4.8.0",
    "@oxc-project/runtime": "^0.113.0",
    "fs-extra": "^11.3.3",
    "jsondiffpatch": "^0.7.3",
    "listr2": "^10.1.0",
    "renovate": "^43.15.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@cenk1cenk2/cz-cc": "^2.2.4",
    "@cenk1cenk2/eslint-config": "^3.1.74",
    "@tsconfig/node24": "^24.0.4",
    "@types/config": "^3.3.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.13",
    "eslint": "10.0.0",
    "execa": "^9.6.1",
    "globby": "^16.1.0",
    "lint-staged": "^16.2.7",
    "oclif": "^4.22.79",
    "prettier": "^3.8.1",
    "simple-git-hooks": "^2.13.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@cenk1cenk2/cz-cc"
    }
  },
  "packageManager": "pnpm@10.29.3"
}
