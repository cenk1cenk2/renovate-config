    "assignees": [
      "cenk1cenk2"
    ],
    "labels": [
      "renovate"
    ],
    "major": {
      "minimumReleaseAge": "3 days"
    },
    "prCreation": "immediate"
  },
  "lock-file": {
    "lockFileMaintenance": {
      "enabled": true,
      "automerge": true,
      "commitMessageSuffix": "[skip ci]",
      "labels": [
        "renovate",
        "lock"
      ],
      "schedule": [
        "before 1am"
      ]
    }
  },
  "no-tests": {
    "ignoreTests": true
  },
  "branch-develop": {
