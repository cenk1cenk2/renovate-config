  "terraform-custom-manager": {
    "customManagers": [
      {
        "depTypeTemplate": "terraform-manager-git-monorepo",
        "customType": "regex",
        "managerFilePatterns": [
          "/\\.tf$/"
        ],
        "matchStringsStrategy": "any",
        "matchStrings": [
          "\"git::git@(?<registryUrl>[^:]*):(?<packageName>[^.]*)(\\.git)?\\/\\/.*\\?ref=(?<depName>.*)@(?<currentValue>[^\"]+)\""
        ],
        "extractVersionTemplate": "^{{{depName}}}@(?<version>.*)$",
        "registryUrlTemplate": "https://{{{registryUrl}}}",
        "datasourceTemplate": "gitlab-tags",
        "versioningTemplate": "semver"
      },
      {
        "depTypeTemplate": "terraform-manager-git-monorepo",
        "customType": "regex",
        "managerFilePatterns": [
          "/\\.tf$/"
        ],
        "matchStringsStrategy": "any",
        "matchStrings": [
          "\"git::git@(?<registryUrl>[^:]*):(?<packageName>[^.]*)(\\.git)?\\/\\/(?<depName>[^\"]+)\""
        ],
        "currentValueTemplate": "0.0.0",
        "extractVersionTemplate": "^{{{depName}}}@(?<version>.*)$",
        "registryUrlTemplate": "https://{{{registryUrl}}}",
        "datasourceTemplate": "gitlab-tags",
        "versioningTemplate": "semver"
      }
    ]
  },
